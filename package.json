{
  "name": "nuxt3-appwrite",
  "version": "1.6.7",
  "description": "Nuxt 3 Appwrite integration",
  "author": {
    "name": "Etienne Moureton",
    "email": "hello@etiennemoureton.fr"
  },
  "homepage": "https://nuxt-appwrite.etiennemoureton.fr",
  "repository": {
    "type": "git",
    "url": "https://github.com/YsarocK/nuxt-appwrite"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "echo \"No test specified\"",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "appwrite": "^10.2.0",
    "node-appwrite": "^8.2.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.1.2",
    "@nuxt/test-utils": "^3.1.2",
    "changelogen": "^0.5.3",
    "eslint": "^8.33.0",
    "nuxt": "^3.6.5",
    "vitest": "^0.34.4"
  },
  "engines": {
    "node": "18.x"
  }
}
